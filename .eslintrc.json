{
    "root": true,
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "extraFileExtensions" : [
            ".vue"
        ]
    },
    "env": {
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:vue/vue3-recommended",
        "prettier"
    ],
    "plugins": [
        // "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "no-undef":"off"
    }
}
